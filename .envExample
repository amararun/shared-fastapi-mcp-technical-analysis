# Gemini API key (required if using Gemini directly)
GEMINI_API_KEY=your_gemini_api_key_here

# Gemini model name (default: gemini-1.5-flash-latest)
GEMINI_MODEL_NAME=gemini-2.0-flash-001

# OpenRouter API key (alternative to Gemini direct â€” supports multiple LLM providers)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# OpenRouter model (default: google/gemini-2.5-flash-preview-09-2025)
OPENROUTER_MODEL=google/gemini-2.5-flash-preview-09-2025

# Rate limiting (defaults shown)
RATE_LIMIT=15/hour
GLOBAL_RATE_LIMIT=100/minute

# Concurrency limits (defaults shown)
MAX_CONCURRENT_PER_IP=2
MAX_CONCURRENT_GLOBAL=4
